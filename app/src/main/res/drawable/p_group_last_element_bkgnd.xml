<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="-2dp"
        android:bottom="2dp"
        android:left="4dp"
        android:right="4dp" >
        <shape android:shape="rectangle">
            <!--<stroke android:color="@color/propertyListGroupBorderColor" android:width="1dp"/>-->
            <solid android:color="@color/propertyListGroupBackgroundColor"/>
            <corners
                android:bottomLeftRadius="10dp"
                android:bottomRightRadius="10dp" />
        </shape>
    </item>
    <item
        android:top="6dp"
        android:bottom="6dp"
        android:left="10dp"
        android:right="10dp" >
        <shape android:shape="rectangle">
            <solid android:color="@color/propertyListElementInGroupColor"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>

</layer-list>